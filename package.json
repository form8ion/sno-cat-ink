{
  "name": "@form8ion/sno-cat-ink",
  "description": "Ink UI for sno-cat",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "example.js",
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "repository": "form8ion/sno-cat-ink",
  "bugs": "https://github.com/form8ion/sno-cat-ink/issues",
  "homepage": "https://npm.im/@form8ion/sno-cat-ink",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "scripts": {
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "pregenerate:md": "npm run build",
    "lint:sensitive": "ban",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "lint:peer": "npm ls >/dev/null",
    "preexample": "npm run build:js",
    "example": "babel-node ./example.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/node": "^7.10.5",
    "@babel/register": "7.10.5",
    "@form8ion/babel-preset": "1.6.28",
    "@form8ion/commitlint-config": "1.0.11",
    "@form8ion/eslint-config": "1.6.26",
    "@form8ion/eslint-config-mocha": "1.2.8",
    "@form8ion/eslint-config-react": "^1.15.22",
    "@form8ion/remark-lint-preset": "1.0.9",
    "@rollup/plugin-babel": "^5.1.0",
    "@travi/any": "2.0.10",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "ban-sensitive-files": "1.9.8",
    "chai": "4.2.0",
    "codecov": "3.7.2",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "7.0.4",
    "lockfile-lint": "4.3.7",
    "mocha": "8.1.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "react-dom": "^16.13.1",
    "remark-cli": "8.0.1",
    "remark-toc": "7.0.0",
    "remark-usage": "8.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.23.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "9.0.2"
  },
  "dependencies": {
    "ink": "3.1.0",
    "react": "^16.13.1"
  }
}
